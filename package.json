{
  "name": "react-router-upgrade",
  "version": "1.0.0",
  "description": "Upgrade React and React-Router to latest versions",
  "main": "index.js",
  "scripts": {
    "watch": "watchify -d app/browser.js -o dist/bundle.js -v",
    "build": "browserify -d app/browser.js > dist/bundle.js",
    "start": "babel-node server"
  },
  "author": "Nigel Reoch",
  "license": "ISC",
  "devDependencies": {
    "babel": "5.5.6",
    "babelify": "6.1.2",
    "browserify": "12.0.1",
    "express": "4.13.3",
    "hbs": "4.0.0",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-addons-update": "0.14.7",
    "react-router": "2.0.0",
    "watchify": "3.6.1"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "dependencies": {
    "babel-polyfill": "6.5.0",
    "flux": "2.1.1",
    "object-assign": "4.0.1",
    "superagent": "1.6.1",
    "react-router": "2.0.0",
    "whatwg-fetch": "0.11.0"
  }
}
